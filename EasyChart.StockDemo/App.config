<?xml version="1.0"?>
<configuration>
  <configSections>
    <sectionGroup name="common">
      <section name="logging" type="Common.Logging.ConfigurationSectionHandler, Common.Logging" requirePermission="false" />
    </sectionGroup>
  </configSections>
  
  <appSettings>
		<add key="ChartControl.DefaultFormulas" value="MAIN#AreaBB#MA(50)#MA(200);VOLMA;RSI(14)#RSI(28);RawData(CLOSE);SlowSTO"/>
		<add key="ChartControl.FavoriteFormulas" value="VOLMA;AreaRSI;CCI;OBV;ATR;FastSTO;SlowSTO;ROC;TRIX;WR;AD;CMF;PPO;StochRSI;ULT;BBWidth;PVO"/>
		<add key="ChartControl.ShowTopLine" value="True"/>
		<add key="ChartControl.Skin" value="GreenRed"/>
		<add key="ChartControl.AreaCount" value="5"/>
		<add key="ChartControl.ShowCursorLabel" value="True"/>
		<add key="ChartControl.ColumnWidth" value="5"/>
		<add key="ChartControl.OverlayFormulas" value="Extend.BB;NATIVE.MA(50);NATIVE.MA(200)"/>
		<add key="ChartControl.ShowCrossCursor" value="True"/>
		<add key="ChartControl.ShowStatistic" value="True"/>
		<add key="ChartControl.ShowIndicatorValues" value="True"/>
		<add key="ChartControl.ShowOverlayValues" value="True"/>
		<add key="ChartControl.AdjustData" value="True"/>
		<add key="tbUserName.Text" value="1"/>
		<add key="tbPassword.Text" value="1"/>
		<add key="ChartControl.AreaPercent" value="3;1;1;1;1"/>
	</appSettings>


  
<startup>
  <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
</startup>

  <common>
    <logging>
      <factoryAdapter type="Common.Logging.NLog.NLogLoggerFactoryAdapter, Common.Logging.NLog31">
        <arg key="configType" value="FILE" />
        <arg key="configFile" value="~/config/log.cfg" />
      </factoryAdapter>
    </logging>
  </common>

  <runtime>
    <gcConcurrent enabled="true" />
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <publisherPolicy apply="yes" />
      <probing privatePath="RequiredLibraries" />
    </assemblyBinding>
  </runtime>
  
</configuration>
